{
  "name": "back_movies",
  "version": "1.0.0",
  "description": "Back movies 2023",
  "main": "server.js",
  "scripts": {
    "start": "node dist/src/server",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "build-ts": "tsc",
    "build": "rm -rf dist/ && npm run build-ts && npm run lint",
    "watch-node": "nodemon dist/src/server.js",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c  \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose"
  },
  "dependencies": {
    "async": "^3.2.2",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "connect-mongo": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^7.1.1",
    "mongoose-unique-validator": "^4.0.0",
    "node": "^20.1.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pug": "^3.0.2",
    "underscore": "^1.13.6"
  },
  "devDependencies": {
    "@types/async": "^3.2.5",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/concurrently": "^5.2.1",
    "@types/cors": "^2.8.13",
    "@types/errorhandler": "1.5.0",
    "@types/eslint": "8.37.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.170",
    "@types/mongodb": "3.6.5",
    "@types/node": "^20.1.3",
    "@types/passport": "^1.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/pug": "^2.0.6",
    "@types/request": "^2.48.5",
    "@types/request-promise": "^4.1.47",
    "@types/supertest": "^2.0.10",
    "@types/underscore": "^1.11.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "concurrently": "^6.0.2",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.7",
    "supertest": "^6.3.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "David Fernando Valenzuela Pardo",
  "license": "ISC"
}
